{
  "name": "@kcdesign/data",
  "version": "0.1.3-experimental.zrx.7",
  "private": false,
  "description": "kcdesign core module",
  "author": "kcaisoft.com",
  "license": "ISC",
  "scripts": {
    "dev": "webpack --config ./webpack.config.js",
    "build": "cross-env NODE_ENV='production' webpack --config ./webpack.config.js",
    "lint": "vue-cli-service lint",
    "test": "jest"
  },
  "dependencies": {
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/chai": "^4.3.1",
    "@types/debug": "^4.1.7",
    "@types/jest": "^28.1.3",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "chai": "^4.3.6",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.0.5",
    "ts-loader": "~8.2.0",
    "typedoc": "^0.22.17",
    "typescript": "~4.5.5",
    "webpack-cli": "^5.0.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {},
    "extends": [
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@typescript-eslint/parser"
    },
    "rules": {
      "no-mixed-spaces-and-tabs": 0,
      "no-useless-escape": 0,
      "no-unused-vars": 0,
      "no-debugger": "off"
    }
  },
  "eslintIgnore": [
    "**/vendor/raphael/*.js"
  ],
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "keywords": [],
  "productName": "kcdesign-core",
  "jest": {
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleNameMapper": {
      "@/(.*)$": "<rootDir>/$1"
    },
    "testEnvironment": "node",
    "transformIgnorePatterns": ["<rootDir>/node_modules/"],
    "preset": "ts-jest",
    "transform": {
        "^.+\\.ts?$": "ts-jest"
    }
  }
}
