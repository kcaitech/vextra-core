{
    "name": "@kcaitech/vextra-core",
    "version": "1.0.4",
    "private": false,
    "type": "module",
    "description": "kcaitech vextra core module",
    "author": "kcaitech.com",
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/types/index.d.ts",
    "license": "AGPL-3.0",
    "files": [
        "dist"
    ],
    "keywords": [
        "vextra",
        "vextra-core",
        "figma",
        "sketch"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/kcaitech/vextra-core.git"
    },
    "publishConfig": {
        "registry": "https://registry.npmjs.org/",
        "access": "public"
    },
    "scripts": {
        "dev": "tsc -w",
        "build": "rollup --config",
        "test": "jest",
        "eslint": "eslint --ext .js,.ts,.vue src",
        "schema": "tsx src/schema/script"
    },
    "dependencies": {
        "@kcaitech/path": "^0.1.4",
        "fzstd": "^0.1.1",
        "jszip": "^3.10.1",
        "kiwi-schema": "^0.5.0",
        "pako": "^2.1.0",
        "uuid": "^9.0.0"
    },
    "devDependencies": {
        "@rollup/plugin-babel": "^6.0.4",
        "@rollup/plugin-json": "^6.1.0",
        "@rollup/plugin-terser": "^0.4.4",
        "@rollup/plugin-typescript": "^11.1.6",
        "@types/chai": "4.3.6",
        "@types/debug": "^4.1.7",
        "@types/jest": "^28.1.3",
        "@types/node": "^22.15.29",
        "@types/pako": "^2.0.3",
        "@types/uuid": "^9.0.1",
        "@types/uzip": "^0.20201231.2",
        "@typescript-eslint/eslint-plugin": "^5.4.0",
        "@typescript-eslint/parser": "^5.4.0",
        "chai": "^4.3.6",
        "eslint": "^7.32.0",
        "eslint-import-resolver-typescript": "^3.6.0",
        "eslint-plugin-import": "^2.27.5",
        "graphviz": "^0.0.9",
        "jest": "^29.5.0",
        "rollup": "^4.18.0",
        "rollup-plugin-clear": "^2.0.7",
        "ts-jest": "^29.0.5",
        "tslib": "^2.8.1",
        "tsx": "^4.11.0",
        "typescript": "^5.8.3"
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not dead",
        "not ie 11"
    ],
    "productName": "vextra-core",
    "jest": {
        "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
        "moduleNameMapper": {
            "@/(.*)$": "<rootDir>/$1"
        },
        "testEnvironment": "node",
        "transformIgnorePatterns": [
            "<rootDir>/node_modules/"
        ],
        "preset": "ts-jest",
        "transform": {
            "^.+\\.ts?$": "ts-jest"
        },
        "testPathIgnorePatterns": [
            "<rootDir>/dist/"
        ],
        "maxConcurrency": 10,
        "maxWorkers": 1
    }
}
